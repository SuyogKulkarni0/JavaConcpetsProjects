
public class CompileTimeBind {
	void add(int a,int b)
	{
		int c = a+b;
		System.out.println("Result is : "+c);
	}
	void add(float a,float b)
	{
		float c = a+b;
		System.out.println("Result in Overloaded Method is : "+c);
	}
	void add(int a,int b,int c)
	{
		int d = a+b+c;
		System.out.println("Result in Overloaded Method 3 is : "+d);
	}
	public static void main(String args[])
	{
		CompileTimeBind c = new CompileTimeBind();
		c.add(10, 20);
		c.add(2.3f, 4.5f);
		c.add(20,30, 40);
		
	}
}
