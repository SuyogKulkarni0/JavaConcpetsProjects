import java.util.*;
public class DivisionException{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a,b,c;
		a = sc.nextInt();
		b = sc.nextInt();
		try {
			c = a/b;
		}
		catch(ArithmeticException e)
		{
			System.out.println("The Division by zero is not Possible here !!");
		}
	}
}